{
	"v": "1.0 dev",
	"name": "Layout",
	"author": "chiririll",
	"created": "13.08.2021",
	"size": [900, 300],
	"localization": {},
	"colors": {
		"_BG": "#000000",
		"icon": "#00ff4b",
		"msg": "#ff00e9",
		"name": "#2000ff",
		"desc": "#d70000"
	},
	"layers": [
		{
			"@type": "Rectangle",
			"xy": [0, 0, 300, 300],
			"fill": "@color/icon"
		},
		{
			"@type": "Rectangle",
			"xy": [310, 10, 890, 100],
			"fill": "@color/msg"
		},
		{
			"@type": "Rectangle",
			"xy": [310, 100, 890, 200],
			"fill": "@color/name"
		},
		{
			"@type": "Rectangle",
			"xy": [310, 200, 890, 290],
			"fill": "@color/desc"
		},

		{
			"@type": "Text",
			"@text": "<Icon>",
			"@box": [0, 0, 300, 300],
			"@font_color": "#ffffff",
			"@font_size": 50
		},
		{
			"@type": "Text",
			"@text": "<Message>",
			"@box": [310, 10, 890, 100],
			"@font_color": "#ffffff",
			"@font_size": 50
		},
		{
			"@type": "Text",
			"@text": "<Name>",
			"@box": [310, 100, 890, 200],
			"@font_color": "#ffffff",
			"@font_size": 50
		},
		{
			"@type": "Text",
			"@text": "<Description>",
			"@box": [310, 200, 890, 290],
			"@font_color": "#ffffff",
			"@font_size": 50
		}
	]
